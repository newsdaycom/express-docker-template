{
  "name": "express-docker-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./app.js",
    "start:dev": "nodemon -L ./app.js"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "arrowParens": "avoid"
  },
  "type": "module",
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "^2.0.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.15"
  }
}
